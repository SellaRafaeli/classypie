<index id='indexPage'>
  <header class="header-2">
                    <div class="container">
                        <div class="row">
                            <div class="navbar col-sm-12 navbar-fixed-top" role="navigation">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle"></button>
                                    <a class="brand" href="/">
                                    <!-- <img src="img/logo@2x.png" width="50" height="50" alt=""> -->
                                    ClassyPie</a>
                                </div>
                                <div class="collapse navbar-collapse">
                                    <ul class="nav pull-right">
                                        <!-- <li class="active"><a href="#">Home</a></li> -->
                                        <% if user_id %>
                                        <li>
                                            <a href=<%='/user/'+user_id%> id='currentUserEmail'> <%= user_email %> </a>                                            
                                        </li>
                                        <li><a href="#" id='logoutBtn' onclick='logout()'>Logout</a></li>
                                        <% else %>
                                        <li><a href="#" id='loginBtn' onclick='signup()'>Login</a></li>
                                        <% end %>
                                    </ul>
                                    <!-- <ul class="subnav">
                                        <li>
                                            <a href="#">Privacy</a>
                                        </li>
                                        <li>
                                            <a href="#">Terms</a>
                                        </li>
                                        <li>
                                            <a href="#">Advertise</a>
                                        </li>
                                        <li>
                                            <a href="#">Affiliates</a>
                                        </li>
                                        <li>
                                            <a href="#">Newsletter</a>
